{
  "name": "meteor-guide",
  "version": "1.0.0",
  "private": true,
  "hexo": {
    "version": "3.9.0"
  },
  "devDependencies": {
    "chexo": "1.0.7",
    "hexo": "3.9.0",
    "hexo-prism-plus": "1.1.0",
    "hexo-renderer-ejs": "1.0.0",
    "hexo-renderer-less": "0.2.0",
    "hexo-renderer-marked": "2.0.0",
    "hexo-server": "1.0.0",
